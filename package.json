{
  "name": "mssu-connect-backend",
  "version": "1.0.0",
  "description": "MSSU Connect - Authentication & User Management Backend",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --runInBand",
    "test:unit": "node --experimental-vm-modules node_modules/jest/bin/jest.js tests/unit --runInBand",
    "test:integration": "node --experimental-vm-modules node_modules/jest/bin/jest.js tests/integration --runInBand",
    "test:security": "node --experimental-vm-modules node_modules/jest/bin/jest.js tests/security --runInBand",
    "test:performance": "node tests/performance/load-test.js",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage --runInBand",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "migrate": "node src/scripts/migrate.js up",
    "migrate:down": "node src/scripts/migrate.js down",
    "migrate:production": "node src/scripts/migrate-production.js",
    "seed": "node src/scripts/seed.js up",
    "seed:down": "node src/scripts/seed.js down",
    "add-user": "node src/scripts/add-user.js",
    "test:models": "node src/scripts/test-models.js",
    "verify:models": "node src/scripts/verify-models.js",
    "test:neon": "node src/scripts/test-neon-connection.js",
    "test:redis": "node src/scripts/test-redis-services.js",
    "test:external": "node src/scripts/test-external-services.js",
    "test:config": "node src/scripts/test-config.js"
  },
  "keywords": [
    "authentication",
    "user-management",
    "express",
    "postgresql",
    "redis"
  ],
  "author": "MSSU Connect Team",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.927.0",
    "@aws-sdk/client-ses": "^3.928.0",
    "@aws-sdk/lib-storage": "^3.927.0",
    "@neondatabase/serverless": "^1.0.0",
    "@sentry/node": "^10.25.0",
    "@sentry/profiling-node": "^10.25.0",
    "bcrypt": "^6.0.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dot": "^1.1.3",
    "dotenv": "^17.2.3",
    "env": "^0.0.2",
    "express": "^5.1.0",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.0",
    "helmet": "^8.1.0",
    "i": "^0.3.7",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "npm": "^11.6.2",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "rate-limit-redis": "^4.2.3",
    "redis": "^5.9.0",
    "sequelize": "^6.37.7",
    "sharp": "^0.34.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "twilio": "^5.10.4",
    "umzug": "^3.8.1",
    "uuid": "^13.0.0",
    "winston": "^3.18.3",
    "winston-cloudwatch": "^6.3.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@types/jest": "^30.0.0",
    "jest": "^30.2.0",
    "nodemon": "^3.1.10",
    "sqlite3": "^5.1.7",
    "supertest": "^7.1.4"
  }
}
